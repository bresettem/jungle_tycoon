<header>
  <!-- Navbar -->
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top">
    <%= link_to 'Jungle Tycoon Cub (Rugged)', root_path, class: 'navbar-brand', data: { turbo: false } %>
    <div class="container-fluid">
      <%= form_with(url: images_path, method: :get, class: 'd-flex', data: { controller: "search-form", turbo_frame: "images" }) do |f| %>
        <div class="form-outline">
          <%= f.text_field :query, id: 'query', class: 'form-control', 'aria-label': 'Search', data: { action: "input->search-form#search" } %>
          <%= f.label 'Search', for: 'query', class: 'form-label' %>
        </div>
      <% end %>
    </div>
  </nav>
</header>