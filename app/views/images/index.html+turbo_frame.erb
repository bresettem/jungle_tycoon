<%= turbo_frame_tag "page_handler" do %>
  <%= turbo_stream_action_tag(
        "append",
        target: "images",
        template: %(#{render @images})
      ) %>
  <%= turbo_stream_action_tag(
        "replace",
        target: "pager",
        template: %(#{render "shared/pager", pagy: @pagy})
      ) %>
<% end %>